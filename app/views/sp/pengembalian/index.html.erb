<div class="table-responsive">


  <table class="table table-striped table-bordered">
    <a href="/sp/pengembalian/baru" class="btn btn-primary">Baru</a>
    <thead>
      <tr>
        <th style="text-align:center">Aksi</th>
        <th style="text-align:center">#</th>
        <th style="text-align:center">ID Peminjaman</th>
        <th style="text-align:center">Tanggal Kembali</th>
        <th style="text-align:center">Denda</th>



      </tr>
    </thead>
    <tbody>

      <% @datas.each do |f| %>
      <tr>
        <td>
          <a href="<%= url_for action: 'view', pengembalian_id: f.id %>" class="btn btn-small btn-default">View</a>
          <a href="<%= url_for action: 'edit', pengembalian_id: f.id %>" class="btn btn-small btn-success">Edit</a>
          <%= link_to 'hapus',url_for(action: :hapus, pengembalian_id: f.id), method: :delete, data: {confirm: "Apakah Anda yakin ingin menghapus data tersebut?"}, class: 'btn btn-small btn-danger' %>
        </td>
        <td><%= f.id %></td>
        <td><%= f.pengembalian_idpinjaml %></td>
        <td><%= f.pengembalian_tanggal %></td>
        <td><%= f.pengembalian_denda %></td>
      
      </tr>
      <% end %>


    </tbody>
  </table>
</div>

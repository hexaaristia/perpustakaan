<%= form_for(@data, url: url_for(action: 'baru_post'), multipart: true ) do |f| %>
<div class="form-group">
  <div class="row">
    <%= f.label 'Judul Buku', { class: 'col-md-2' } %>
    <div class="col-md-8">
        <%= f.text_field :buku_judul, { class: 'form-control', placeholder: '' } %>
    </div>
  </div>
</div>

<div class="form-group">
  <div class="row">
    <%= f.label 'Penulis', { class: 'col-md-2' } %>
    <div class="col-md-8">
        <%= f.text_field :buku_penulis, { class: 'form-control', placeholder: '' } %>
    </div>
  </div>
</div>

<div class="form-group">
  <div class="row">
    <%= f.label 'Penerbit', { class: 'col-md-2' } %>
    <div class="col-md-8">
        <%= f.text_field :buku_penerbit, { class: 'form-control', placeholder: '' } %>
    </div>
  </div>
</div>

<div class="form-group">
  <div class="row">
    <%= f.label 'Tahun Terbit', { class: 'col-md-2' } %>
    <div class="col-md-8">
        <%= f.text_field :buku_thnterbit, { class: 'form-control', placeholder: '' } %>
    </div>
  </div>
</div>

<div class="form-group">
  <input type="submit" value="Masukan" class="btn btn-primary">
</div>
<% end %>
